package basico.datas.calendar;

import java.time.DayOfWeek;
import java.util.Calendar;

public class Exercicio {
	public static void main(String[] args) {
		Calendar dataDeVencimento =  Calendar.getInstance();
		
		Calendar a = dataDeVencimento;
		dataDeVencimento.add(Calendar.DATE, 10);
	
		int i = diaDaSemana(dataDeVencimento);
		System.out.println(i);
	}
	
	public static int diaDaSemana(Calendar x) 
	{
		return Calendar.DAY_OF_WEEK;
	}
}
