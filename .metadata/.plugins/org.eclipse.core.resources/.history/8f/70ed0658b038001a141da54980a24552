package basico.datas.calendar;

import java.util.Calendar;

public class Exercicio {
	public static void main(String[] args) {
		Calendar dataDeVencimento =  Calendar.getInstance();
		
		Calendar a = dataDeVencimento;
		dataDeVencimento.add(Calendar.DATE, 3);
		a.add(Calendar.DATE, 10);
		
		
		System.out.println(dataDeVencimento.getTime());
		System.out.println(dataDeVencimento.SUNDAY);
		System.out.println(dataDeVencimento.MONDAY);
		System.out.println(dataDeVencimento.TUESDAY);
		System.out.println(dataDeVencimento.WEDNESDAY);
		
		System.out.println(dataDeVencimento.equals(a.SUNDAY));
		
	}
}
